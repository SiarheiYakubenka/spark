#!groovy

properties([disableConcurrentBuilds()])


pipeline {
    agent any

    options{
    timestamps()
     }

    stages {

        stage('Compile') {
            steps {
                echo "Compiling..."
                bat "sbt compile"
            }
        }

        stage('Test') {
            steps {
                echo "Testing..."
                bat "sbt test"
            }
        }

    }
}